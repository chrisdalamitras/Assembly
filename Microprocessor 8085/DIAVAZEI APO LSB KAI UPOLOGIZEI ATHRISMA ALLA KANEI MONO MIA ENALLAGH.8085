IN 10H
CALL ARXIK
MVI C,00H
MVI B,00H

START:
	LDA 2000H
	ANI 07H
	MOV E,A
	STA 08D5H

DEIXE:
	LXI D,08D0H
	CALL STDM
	CALL DCD


START1:
	LDA 2000H
	ANI 07H
	MOV E,A
	STA 08D5H
	JMP SUNEXISE1
	

SUNEXISE1:
	CALL KIND
	CPI 85H
	JZ FUGE
	MOV C,A
SUNEXISE2:
	LDA 2000H
	ANI 07H
	MOV E,A
	STA 08D5H
	MOV A,C
	CPI 09H
	JNC SUNEXISE1
	ADD E
	STA 08D0H
	JMP DEIXE1

FUGE:
	LDA 2000H
	ANI 07H
	CMP C
	JZ SUNEXISE2
FUGE1:
	LDA 2000H
	ANI 07H
	MOV E,A
	STA 08D0H
	MOV A,C
	CPI 09H
	JNC SUNEXISE1
	ADD E
	STA 08D5H
	MOV C,A
	JMP DEIXE1



DEIXE1:
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START1




ARXIK:
	MVI A,10H
	STA 08D0H
	STA 08D1H
	STA 08D2H
	STA 08D3H
	STA 08D4H
	STA 08D5H
	RET

END