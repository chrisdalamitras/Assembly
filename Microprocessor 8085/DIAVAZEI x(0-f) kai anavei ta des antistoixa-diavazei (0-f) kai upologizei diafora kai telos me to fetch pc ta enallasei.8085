IN 10H
CALL ARXIK
MVI C,00H
MVI C,00H
START:
	LDA 2000H
	CPI 80H
	JNC SUNEXA1
START1:
	LDA 09C0H
	STA 09C2H
	CALL KIND
	CPI 0FH
	JNC START
	CPI 0DH
	JZ DEIXE
	CPI 0BH
	JZ DEIXE
	CPI 09H
	JZ DEIXE
	CPI 07H
	JZ DEIXE
	CPI 05H
	JZ DEIXE
	CPI 03H
	JZ DEIXE
	CPI 01H
	JZ DEIXE
	JMP START
DEIXE:
	STA 09C0H
	ANI 0FH
	CMA
	STA 3000H
	JMP START

ALLAGH:
	LDA 09C4H
	STA 08D5H
	MVI A,10H
	STA 08D0H
	JMP EKTUPWSE	


SUNEXA1:
	LDA 09C2H
	MOV E,A
	MOV A,C
	CPI 00H
	JNZ SUNEXA2
	MOV A,E
	MVI C,01H
	JMP SUNEXA
SUNEXA2:
	CALL KIND
	CPI 85H
	JZ ALLAGH
SUNEXA:
	CPI 0FH
	JNC SUNEXA2
	STA 09C1H
	LDA 09C0H
	MOV D,A
	LDA 09C1H
	CMP D
	JC ANAPODA
	SUB D
	STA 09C4H
	STA 08D0H
	MVI A,10H
	STA 08D5H
	JMP EKTUPWSE

ANAPODA:
	LDA 09C1H
	MOV D,A
	LDA 09C0H
	SUB D
	STA 09C4H
	STA 08D0H
	LXI D,08D0H
	JMP EKTUPWSE

	
EKTUPWSE:
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START



ARXIK:
	MVI A,10H
	STA 08D0H
	STA 08D1H
	STA 08D2H
	STA 08D3H
	STA 08D4H
	STA 08D5H
	RET

END