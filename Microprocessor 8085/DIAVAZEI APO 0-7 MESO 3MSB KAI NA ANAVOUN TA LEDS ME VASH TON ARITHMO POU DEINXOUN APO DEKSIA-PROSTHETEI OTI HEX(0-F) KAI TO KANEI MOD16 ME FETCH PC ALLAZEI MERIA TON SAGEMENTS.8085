IN 10H
MVI B,00H

START:
	MVI A,10H
	CALL ARXIK
	MVI C,00H
	MVI D,00H
	LDA 2000H
	RLC
	RLC
	RLC
	ANI 07H
	CPI 00H
	JZ SUNEXEIA
LOOP1:
	CPI 00H
	JZ SUNEXEIA
	SUI 01H
	MOV D,A
	MOV A,C
	ADI 01H
	RLC
	MOV C,A
	MOV A,D
	JMP LOOP1
SUNEXEIA:
	MOV A,C
	RRC
	CMA
	STA 3000H
PERIMENE:
	CALL KIND
	CPI 85H
	JC DENALLAZEIS
	MOV A,B
	STA 08D5H
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START
DENALLAZEIS:
	CPI 10H
	JNC PERIMENE
	MOV D,A
	LDA 2000H
	RLC
	RLC
	RLC
	ANI 07H
	ADD D
	CPI 10H
	JC SUNEXISE
	SUI 10H
SUNEXISE:
	MOV B,A
	STA 08D0H
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START
	
ARXIK:
	STA 08D0H
	STA 08D1H
	STA 08D2H
	STA 08D3H
	STA 08D4H
	STA 08D5H
	RET
END