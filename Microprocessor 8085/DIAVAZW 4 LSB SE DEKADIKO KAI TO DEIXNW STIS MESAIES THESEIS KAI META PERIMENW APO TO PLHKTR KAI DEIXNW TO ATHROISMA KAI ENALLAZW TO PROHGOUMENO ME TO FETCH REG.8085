IN 10H
CALL ARXIK
MVI A,00H
STA 09C0H


START:
	MVI C,00H
	LDA 2000H
	ANI 0FH
	CPI 0AH
	JNC DEC
	JMP LEDS

DEC:
	CPI 0AH
	JC LEDS
	INR C
	SUI 0AH
	JMP DEC

LEDS:
	STA 08D2H
	MOV A,C
	STA 08D3H
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP NEXT

NEXT:
	CALL KIND
	CPI 80H
	JZ ALLAGH
	CPI 0EH
	JNC NEXT
	MOV D,A
	LDA 09C0H
	STA 09C1H
	ADD D
	CPI 10H
	JNC NEXT2
NEXT1:
	STA 09C0H
	STA 08D0H
	JMP EKTUP

NEXT2:
	SUI 10H
	JMP NEXT1

ALLAGH:
	LDA 09C1H
	STA 08D0H
	STA 09C2H
	LDA 09C0H
	STA 09C1H
	LDA 09C2H
	STA 09C0H
	JMP EKTUP

EKTUP:
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START


ARXIK:
	MVI A,10H
	STA 08D0H
	STA 08D1H
	STA 08D2H
	STA 08D3H
	STA 08D4H
	STA 08D5H
	RET

END
