IN 10H
CALL ARXIK
MVI C,00H


START:
	CALL KIND
	STA 08D5H
	ADD C
	MOV C,A
	STA 08D0H
	CPI 10H
	JNC YPOLOGISMOS
	JMP EKSODOS
YPOLOGISMOS:
	MVI B,00H
YPOL:
	INR B
	SUI 10H
	CPI 10H
	JNC YPOL
	JMP EKSODOS1

EKSODOS:
	LXI D,08D0H
	CALL STDM
	CALL DCD
	JMP START

EKSODOS1:
	STA 08D0H
	MOV A,B
	STA 08D1H
	JMP EKSODOS
	

ARXIK:
	MVI A,10H
	STA 08D0H
	STA 08D1H
	STA 08D2H
	STA 08D3H
	STA 08D4H
	STA 08D5H
	RET

END